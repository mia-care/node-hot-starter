{
  "name": "fastify-starter",
  "version": "2.0.0",
  "description": "A very opinionated Fastify Plugin to kickstart your project",
  "main": "src/index.js",
  "types": "src/types/index.d.ts",
  "scripts": {
    "test": "npm run lint && npm run unit",
    "unit": "node --test --experimental-test-coverage test/*.test.js test/**/*.test.js",
    "collect-coverage": "NODE_OPTIONS='--test-reporter lcov --test-reporter-destination=lcov.info' npm run unit",
    "coverage:html": "c8 --reporter=html -o /tmp/c8-coverage npm run unit && open /tmp/c8-coverage/index.html || xdg-open /tmp/c8-coverage/index.html",
    "lint": "eslint . --ignore-path=.gitignore",
    "example": "node example/index.js",
    "spellcheck": "cspell '**/*.md'"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mia-care/fastify-starter.git"
  },
  "author": "Mia-Care R&D Team",
  "license": "Apache-2.0",
  "devDependencies": {
    "@mia-platform/eslint-config-mia": "^3.0.0",
    "eslint": "^8.56.0",
    "form-auto-content": "^3.2.1",
    "nock": "^13.5.0",
    "cspell": "^8.7.0",
    "c8": "^10.1.3"
  },
  "dependencies": {
    "deepmerge": "^4.3.1"
  },
  "peerDependencies": {
    "@fastify/env": ">= 5.0.2",
    "@fastify/formbody": ">= 8.0.2",
    "@fastify/swagger": ">= 9.4.2",
    "@fastify/swagger-ui": ">= 5.2.2",
    "axios": ">= 1.8.4",
    "fastify": ">= 5.2.1",
    "fastify-metrics": ">= 12.1.0",
    "fastify-plugin": ">= 5.0.1",
    "pino": ">= 9.6.0"
  },
  "engines": {
    "node": ">=20"
  }
}